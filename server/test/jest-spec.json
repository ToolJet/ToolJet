{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testTimeout": 20000,
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)": "<rootDir>/../src/$1",
    "@plugins/(.*)": "<rootDir>/../plugins/$1",
    "@dto/(.*)": "<rootDir>/../src/dto/$1",
    "@services/(.*)": "<rootDir>/../src/services/$1",
    "@controllers/(.*)": "<rootDir>/../src/controllers/$1",
    "@ee/(.*)": "<rootDir>/../ee/$1"
  }
}