          "dataQueries": [
            {
              "id": "cfa089be-6a4b-41cc-9435-c240ae438ad4",
              "name": "getOptions",
              "options": {
                "code": "function getOptions() {\n  return {\n    passwordLength: components.lengthCheck.value,\n    includeUppercase: components.uppercase.value,\n    includeLowercase: components.lowercase.value,\n    includeNumbers: components.numbers.value,\n    includeSpecial: components.special.value,\n    avoidAmbiguous: components.avoidAmbiguous.value\n  };\n}\nreturn getOptions();\n",
                "parameters": []
              },
              "dataSourceId": "9d3bbeb6-305f-43f0-a5a2-fc3fbc479031",
              "appVersionId": "e613ad0b-081e-4c02-aaa2-7f22871e76bd",
              "createdAt": "2024-10-05T10:42:04.983Z",
              "updatedAt": "2024-10-05T10:42:04.983Z"
            },
            {
              "id": "eed6ee3d-4b0e-4a42-b1de-73c560cdabf7",
              "name": "setPassword",
              "options": {
                "code": " async  function setPassword() {\n   await queries.getOptions.run()\n    await queries.generatePassword.run();\n      const password = queries.generatePassword.getData();\n      components.passwordField.value = password;\n       actions.setVariable('password', password);\n    \n   return password;   \n  };\n\nreturn setPassword();",
                "parameters": [],
                "requestConfirmation": false,
                "showSuccessNotification": false,
                "successMessage": "Password Generated Successfully!",
                "notificationDuration": ""
              },
              "dataSourceId": "9d3bbeb6-305f-43f0-a5a2-fc3fbc479031",
              "appVersionId": "e613ad0b-081e-4c02-aaa2-7f22871e76bd",
              "createdAt": "2024-10-05T10:42:04.983Z",
              "updatedAt": "2024-10-05T10:42:04.983Z"
            },
            {
              "id": "e342c99e-d977-4339-a383-ca9f35907830",
              "name": "generatePassword",
              "options": {
                "code": "function generatePassword(options) {\n  const charSets = {\n        uppercase: \"ABCDEFGHIJKLMNOPQRSTUVWXYZ\",\n        lowercase: \"abcdefghijklmnopqrstuvwxyz\",\n        numbers: \"0123456789\",\n        special: \"!@#$%^&*()_+[]{}|;:,.<>?\"\n    };\n\n    const ambiguousChars = /[IlO0]/g;\n\n    let possibleChars = \"\";\n\n    if (options.passwordLength < 1){\n      actions.showAlert('info' , ' Password length must be greater than zero!' )\n    }\n    // Concatenate possible characters based on user options\n    if (options.includeUppercase) {\n        possibleChars += charSets.uppercase;\n    }\n    if (options.includeLowercase) {\n        possibleChars += charSets.lowercase;\n    }\n    if (options.includeNumbers) {\n        possibleChars += charSets.numbers;\n    }\n    if (options.includeSpecial) {\n        possibleChars += charSets.special;\n    }\n\n    // Remove ambiguous characters if needed\n    if (options.avoidAmbiguous) {\n        possibleChars = possibleChars.replace(ambiguousChars, \"\");\n    }\n\n    // Ensure at least one character type is selected\n    if (!possibleChars) {\n        actions.showAlert('error' , 'Please select at least one character type!' )\n        return \"\";\n    }\n\n    const passwordLength = options.passwordLength || 8;\n\n    // Generate password\n    const password = Array.from({ length: passwordLength }, () => {\n        const randomIndex = Math.floor(Math.random() * possibleChars.length);\n        return possibleChars[randomIndex];\n    }).join('');\n\n    return password;\n};\nconst options = queries.getOptions.data;\nconsole.log('password04', options )\nreturn generatePassword(options)\n\n\n",
                "parameters": [],
                "showSuccessNotification": false
              },
              "dataSourceId": "9d3bbeb6-305f-43f0-a5a2-fc3fbc479031",
              "appVersionId": "e613ad0b-081e-4c02-aaa2-7f22871e76bd",
              "createdAt": "2024-10-05T10:42:04.983Z",
              "updatedAt": "2024-10-05T10:42:04.983Z"
            }
