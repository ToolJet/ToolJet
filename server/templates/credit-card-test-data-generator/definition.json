              "id": "fce8089c-6839-4704-b4e1-65b1ed9a2398",
              "name": "runjs2",
              "options": {
                "code": "// Copy the result from result_box to clipboard\nconst resultText = components.result_box.value;\nnavigator.clipboard.writeText(resultText).then(() => {\n    alert('Data copied to clipboard');\n}).catch(err => {\n    console.error('Failed to copy text: ', err);\n});\n",
