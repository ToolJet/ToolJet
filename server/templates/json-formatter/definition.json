                    "dataQueries": [
                        {
                            "id": "9ecf8bbc-f01f-4152-b083-a036fd312015",
                            "name": "runjs1",
                            "options": {
                                "code": "            const input = components.textarea1.value;\n            const indentOption = components.indentOptions.value;\n\n            // Parse the JSON and format it\n            let parsed;\n            try {\n                parsed = JSON.parse(input);\n            } catch (error) {\n                const formattedJSON = \"Invalid JSON input: \" + error.message;\n                return formattedJSON;\n            }\n\n            // Determine the indentation\n            let indentChar;\n            switch (indentOption) {\n                case 'tab':\n                    indentChar = '\\t';\n                    break;\n                case '2spaces':\n                    indentChar = '  ';\n                    break;\n                case '4spaces':\n                    indentChar = '    ';\n                    break;\n                case '8spaces':\n                    indentChar = '        ';\n                    break;\n                default:\n                    const formattedJSON = \"Invalid indent option\";\n                    return formattedJSON;\n            }\n\n            // Format and display the JSON\n            const formattedJSON = JSON.stringify(parsed, null, indentChar);\n            return formattedJSON;",
                                "parameters": []
                            },
                            "dataSourceId": "a63cd93c-d037-43f2-8ec9-2c42f63ab303",
                            "appVersionId": "5d97d50c-75fb-4b6f-bc59-22ee1956c1a4",
