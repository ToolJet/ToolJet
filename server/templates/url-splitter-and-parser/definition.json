          "dataQueries": [
            {
              "id": "d29bcc7b-4c5b-4da1-a44d-960e895fd2c8",
              "name": "parseUrl",
              "options": {
                "code": "const url = components.url_input.value\n\nconst parser = document.createElement(\"a\");\nparser.href = url;\n\n  // Extract individual components\nconst scheme = parser.protocol.replace(\":\", \"\"); // Scheme\n  const protocol = parser.protocol; // Protocol\n  const userinfo = parser.username || parser.password ? `${parser.username}:${parser.password}` : \"\"; // Userinfo\n  const username = parser.username; // Username\n  const password = parser.password; // Password\n  const authority = parser.host; // Authority\n  const host = parser.hostname; // Host\n  const hostname = parser.hostname; // Hostname\n  const subdomain = host.split(\".\").length > 2 ? host.split(\".\")[0] : \"\"; // Subdomain\n  const domain = host.split(\".\").slice(-2).join(\".\"); // Domain\n  const tld = host.split(\".\").slice(-1)[0]; // TLD\n  const resource = parser.pathname; // Resource\n  const directory = parser.pathname.substring(0, parser.pathname.lastIndexOf(\"/\") + 1); // Directory\n  const path = parser.pathname; // Path\n  const fileName = parser.pathname.split(\"/\").pop(); // File Name\n  const fileSuffix = fileName.includes(\".\") ? fileName.split(\".\").pop() : \"\"; // File suffix\n  const queryString = parser.search.substring(1); // Query String\n  const hash = parser.hash.substring(1); // Hash\n\n\nlet result = `\nScheme: ${scheme || \"Not Provided\"}\nProtocol: ${protocol || \"Not Provided\"}\nUserinfo: ${userinfo || \"Not Provided\"}\nUsername: ${username || \"Not Provided\"}\nPassword: ${password || \"Not Provided\"}\nAuthority: ${authority || \"Not Provided\"}\nHost: ${host || \"Not Provided\"}\nHostname: ${hostname || \"Not Provided\"}\nSubdomain: ${subdomain || \"Not Provided\"}\nDomain: ${domain || \"Not Provided\"}\nTLD: ${tld || \"Not Provided\"}\nResource: ${resource || \"Not Provided\"}\nDirectory: ${directory || \"Not Provided\"}\nPath: ${path || \"Not Provided\"}\nFile Name: ${fileName || \"Not Provided\"}\nFile Suffix: ${fileSuffix || \"Not Provided\"}\nQuery String: ${queryString || \"Not Provided\"}\nHash: ${hash || \"Not Provided\"}\n  `.trim();\n\nactions.showAlert(\"success\", \"URL parsed successfully\")\nreturn {result: result || \"\"}\n// return result",
                "parameters": []
              },
              "dataSourceId": "da846a78-3a1b-421a-aa63-619ad2558ca4",
              "appVersionId": "6ab8ff42-1af6-47bc-8457-1f5140d3ef49",
              "createdAt": "2024-11-25T17:05:57.486Z",
              "updatedAt": "2024-11-25T17:05:57.486Z"
            },
            {
              "id": "adf721ce-aa14-4ba5-b21d-1d7bebb814a4",
              "name": "copyToClipboard",
              "options": {
                "code": "const url_data = components.textarea1.value;\n\n// Clean the URL data\nawait queries.stripHtmlTags.run({htmlString: url_data});\nconst clean_url_data = queries.stripHtmlTags.getData();\n\n// Copy cleaned data to clipboard\nactions.copyToClipboard(clean_url_data);\n",
                "parameters": []
              },
              "dataSourceId": "da846a78-3a1b-421a-aa63-619ad2558ca4",
              "appVersionId": "6ab8ff42-1af6-47bc-8457-1f5140d3ef49",
              "createdAt": "2024-11-25T17:05:57.486Z",
              "updatedAt": "2024-11-25T17:05:57.486Z"
            },
            {
              "id": "ed240e5c-4cb7-4903-bb4c-b4fde0dba33f",
              "name": "stripHtmlTags",
              "options": {
                "code": "  const tempDiv = document.createElement(\"div\");\n  tempDiv.innerHTML = parameters.htmlString;\n  return tempDiv.textContent || tempDiv.innerText || \"\";",
                "parameters": [
                  {
                    "name": "htmlString",
                    "defaultValue": "1"
                  }
                ]
              },
              "dataSourceId": "da846a78-3a1b-421a-aa63-619ad2558ca4",
