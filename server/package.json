    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.0",
    "@sentry/node": "^6.12.0",
    "@sentry/tracing": "^6.12.0",
    "@types/got": "^9.6.12",
    "@types/humps": "^2.0.1",
    "@types/nodemailer": "^6.4.4",
    "aws-sdk": "^2.947.0",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "futoin-hkdf": "^1.4.2",
    "got": "^11.8.2",
    "helmet": "^4.6.0",
    "humps": "^2.0.1",
    "ioredis": "^4.27.6",
    "joi": "^17.4.1",
    "knex": "^0.95.7",
    "mongodb": "^4.0.0",
    "mysql": "^2.18.1",
