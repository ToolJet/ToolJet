{
  "name": "@tooljet/plugins",
  "version": "0.0.1",
  "type": "module",
  "source": "server.ts",
  "types": "dist/server.d.ts",
  "main": "dist/server.js",
  "scripts": {
    "create:client": "node build-scripts/create-client-entry",
    "create:server": "node build-scripts/create-server-entry",
    "create:operations": "node build-scripts/create-operations-file",
    "build:server": "rimraf dist/ && tsc -b",
    "start": "tsc -b --watch",
    "build": "npm run create:client && npm run create:server && npm run build:packages && npm run build:server && npm run create:operations",
    "prebuild:packages": "npm run clean:packages",
    "build:packages": "node build-scripts/build-packages.js",
    "clean:packages": "node build-scripts/clean-packages.js",
    "lint": "eslint .  '**/*.ts'",
    "format": "eslint . --fix '**/*.ts'",
    "test": "NODE_ENV=test jest"
  },
  "workspaces": [
    "packages/*"
  ],
